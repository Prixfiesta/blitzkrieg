<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="maybethistime.SignupController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="Signup" layoutX="285.0" layoutY="293.0" mnemonicParsing="false" onAction="#signuphandler" text="Signup" />
            <TextField fx:id="username" layoutX="237.0" layoutY="77.0" promptText="Enter Username" />
            <PasswordField fx:id="password" layoutX="237.0" layoutY="149.0" promptText="Enter Password" />
            <PasswordField fx:id="repassword" layoutX="237.0" layoutY="223.0" promptText="Re-enter Password" />
            <Label layoutX="248.0" layoutY="24.0" text="Sign up">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
